{
  "name": "identity-svc",
  "version": "1.0.0",
  "description": "Welcome to the GitHub repository for **Identity SVC** - This service is responsible for managing user identity and their information. It provides functionality for user registration, authentication, and profile management, allowing users to securely manage their operations.",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon src/index.js",
    "start-appmap": "npx appmap-node nodemon src/index.js",
    "start": "nodemon -r dotenv/config --experimental-json-modules src/index.js",
    "format": "prettier --write \"src/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\"",
    "infra:build": "docker-compose build",
    "infra:up": "docker-compose up -d",
    "infra:down": "docker-compose down",
    "infra:logs": "docker logs -f finance-email-consumer"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ayushya100/identity-svc.git"
  },
  "keywords": [
    "nodejs",
    "express",
    "javascript",
    "identity-svc"
  ],
  "author": "Ayushya Jaiswal",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ayushya100/identity-svc/issues"
  },
  "homepage": "https://github.com/Ayushya100/identity-svc#readme",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.3",
    "jsonwebtoken": "^9.0.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4"
  }
}
